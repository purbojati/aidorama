# Existing environment variables...
DATABASE_URL=your_database_url
OPENROUTER_API_KEY=your_openrouter_api_key
BETTER_AUTH_SECRET=your_auth_secret
BETTER_AUTH_URL=your_auth_url

# Prompt Optimization Configuration
# Set to 'true' to force use of optimized prompts (overrides rollout percentage)
USE_OPTIMIZED_PROMPTS=false

# Percentage of requests to use optimized prompts (0-100)
# Use this for gradual rollout and A/B testing
OPTIMIZED_PROMPT_ROLLOUT=0